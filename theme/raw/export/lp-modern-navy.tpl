{if !$embed}
<!DOCTYPE html>
<html lang="sl">
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<title>Europass LP</title>
{/if}
<style>
{if !$print and !$embed}
{include file="artefact:europass:export/font-face.tpl"}
{/if}

{if !$embed}
html {
	margin: 0;
}

body {
	margin: 3rem 0;
	padding: 0;
	font-family: DejaVu Sans, sans-serif;
	font-size: {if $print}10.5pt{else}9.5pt{/if};
	overflow: visible;
}
{/if}

.header {
	/* padding: top, right, bottom, left */
	padding: 2rem 4rem 2rem 2rem;
	color: #ffffff;
	background-color: #004b80;
	margin-top: -3rem;
}

.header a {
	color: #ffffff;
}

.content {
	/* padding: top, right, bottom, left */
	padding: 0rem 4rem 2rem 2rem;
	padding-left: 12.5rem;
}

.logo {
	float: right;
	width: 10rem;
	padding-left: 1rem;
}

.photo {
	width: 10rem;
}

.profile-photo {
	width: 8rem;
	height: 8rem;
	object-fit: cover;
    border-radius: 50%;
	border: 3px solid #4d9acb;
}

hr {
	border: 1px solid #4d9acb;
	background-color: #4d9acb;
}

h3 {
	text-transform: uppercase;
	border-bottom: 2px solid #4d9acb;
}

h4 {
	border-bottom: 2px dotted #4d9acb;
	padding-top: 0.75rem;
}

.date,
.label,
.skill {
	text-transform: uppercase;
	padding-bottom: 0.15rem;
}

.skill {
	font-weight: bold;
}

.entry {
	padding-bottom: 0.15rem;
}

.details p {
	margin-block-start: 0.15rem;
}

table.info-table {
	border-collapse: collapse;
}

table.info-table td,
table.info-table th {
	padding: 0.25rem;
	border-top: 1px solid #4d9acb;
}

table.info-table tr.info-table-header {
	background-color: #dcecf6;
	font-size: smaller;
}

table.info-table tr.info-table-footer {
	font-size: smaller;
}

@media only screen and (min-width: 1080px) {
  /* For large screens */
  table.info-table {
    width: 75%;
  }
}

@media not only screen and (min-width: 1080px) {
  /* For small screens */
  table.info-table {
    width: 100%;
  }
}
</style>

{if !$embed}
</head>

<body>
{/if}

	<div class="header">
		<table id="profile-content" width="100%" border="0">
		<tr>
			<td class="photo">
				{if $photo}<img class="profile-photo" src="data:image/jpeg;base64,{$photo}">{/if}
			</td>
			<td>
				<img class="logo" style="float:right" src="data:image/png;base64," />
				<h1>{if $prefs.personname == 12}{$profilefields.firstname} {$profilefields.lastname}{else}{$profilefields.lastname} {$profilefields.firstname}{/if}</h1>
				<hr />
				<p>
					{if $skills.mothertongue}<b>{$text.mothertongue}:</b> {foreach from=$skills.mothertongue item=lang name=lg}{$lang->label}{if !$.foreach.lg.last} &bull; {/if}{/foreach} |{/if}
					{if $skills.otherlanguage}<b>{$text.otherlanguage}:</b> {foreach from=$skills.otherlanguage item=lang name=lg}{$lang->label}{if !$.foreach.lg.last} &bull; {/if}{/foreach}{/if}
				</p>
			</td>
		</tr>
		</table>
	</div>

	<div class="content">
	{foreach from=$skills.otherlanguage item=lang}
		<br /><h3>{$lang->label}</h3>

		<p class="entry">
			<table width="100%" class="info-table">
				<tr class="info-table-header">
					<th width="30%">&nbsp;</th>
					<th width="28%" colspan="2">{$text.understanding}</th>
					<th width="28%" colspan="2">{$text.speaking}</th>
					<th width="14%">{$text.writing}</th>
				</tr>
				<tr align="center" class="info-table-header">
					<th width="30%">&nbsp;</th>
					<td width="14%"><small>{$text.listening}</small></td>
					<td width="14%"><small>{$text.reading}</small></td>
					<td width="14%"><small>{$text.spokeninteraction}</small></td>
					<td width="14%"><small>{$text.spokenproduction}</small></td>
					<td width="14%">&nbsp;</td>
				</tr>
				<tr align="center">
					<th width="30%">&nbsp;</th>
					<td width="14%">{$lang->listening}</td>
					<td width="14%">{$lang->reading}</td>
					<td width="14%">{$lang->spokeninteraction}</td>
					<td width="14%">{$lang->spokenproduction}</td>
					<td width="14%">{$lang->writing}</td>
				</tr>
				<tr class="info-table-footer">
					<td colspan="6"><small><i>{$text.cef_level}</i></small></td>
				</tr>
			</table>
		</p>

		{if $lang->certificate}
		<h4>{$text.languagediplomas}</h4>

		{foreach from=$lang->certificate item=cert}
		<p class="entry">
			<div class="date">{$cert->certificatedate|format_date:'strfdaymonthyearshort'}</div>
			<div class="label"><b>{$cert->certificate}</b> &ndash; {$cert->awardingbody}</div>
			<div class="level"><b>{$text.diplomalevel}:</b> {$cert->europeanlevel}</div>
		</p>
		{/foreach}
		{/if}

		{if $lang->experience}
		<h4>{$text.languageexperience}</h4>

		{foreach from=$lang->experience item=exp}
		<p class="entry">
			<div class="date">{$exp->startdate|format_date:'strfdaymonthyearshort'}{if $exp->enddate} &ndash; {$exp->enddate|format_date:'strfdaymonthyearshort'}{else} &rarr;{/if}</div>
			<div class="details">{$exp->experience}</div>
		</p>
		{/foreach}
		{/if}

	{/foreach}
	</div>

{if !$embed}
</body>
</html>
{/if}

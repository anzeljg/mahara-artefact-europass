{if !$embed}
<!DOCTYPE html>
<html lang="sl">
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<title>Europass CV</title>
{/if}
<style>
{if !$print and !$embed}
{include file="artefact:europass:export/font-face.tpl"}
{/if}

{if !$embed}
html {
	margin: 0;
}

body {
	margin: 3rem 0;
	padding: 0;
	font-family: DejaVu Sans, sans-serif;
	font-size: {if $print}10.5pt{else}9.5pt{/if};
	overflow: visible;
}
{/if}

.header {
	/* padding: top, right, bottom, left */
	padding: 2rem 4rem 2rem 2rem;
	color: #ffffff;
	background-color: #004b80;
	margin-top: -3rem;
}

.header a {
	color: #ffffff;
}

.content {
	/* padding: top, right, bottom, left */
	padding: 0rem 4rem 2rem 2rem;
	padding-left: 12.5rem;
}

.logo {
	float: right;
	width: 10rem;
	padding-left: 1rem;
}

.photo {
	width: 10rem;
}

.profile-photo {
	width: 8rem;
	height: 8rem;
	object-fit: cover;
    border-radius: 50%;
	border: 3px solid #4d9acb;
}

hr {
	border: 1px solid #4d9acb;
	background-color: #4d9acb;
}

h3 {
	text-transform: uppercase;
	border-bottom: 2px solid #4d9acb;
}

.date,
.label,
.skill {
	text-transform: uppercase;
	padding-bottom: 0.15rem;
}

.skill {
	font-weight: bold;
}

.entry {
	padding-bottom: 0.15rem;
}

.details p {
	margin-block-start: 0.15rem;
}

table.info-table {
	border-collapse: collapse;
}

table.info-table td,
table.info-table th {
	padding: 0.25rem;
	border-top: 1px solid #4d9acb;
}

table.info-table tr.info-table-header {
	background-color: #dcecf6;
	font-size: smaller;
}

table.info-table tr.info-table-footer {
	font-size: smaller;
}

@media only screen and (min-width: 1080px) {
  /* For large screens */
  table.info-table {
    width: 75%;
  }
}

@media not only screen and (min-width: 1080px) {
  /* For small screens */
  table.info-table {
    width: 100%;
  }
}
</style>

{if !$embed}
</head>

<body>
{/if}

	<div class="header">
		<table id="profile-content" width="100%" border="0">
		<tr>
			<td class="photo">
				{if $photo}<img class="profile-photo" src="data:image/jpeg;base64,{$photo}">{/if}
			</td>
			<td>
				<img class="logo" style="float:right" src="data:image/png;base64," />
				<h1>{if $prefs.personname == 12}{$profilefields.firstname} {$profilefields.lastname}{else}{$profilefields.lastname} {$profilefields.firstname}{/if}</h1>
				<hr />
				<p>
					{if $profilefields.dateofbirth}<b>{$text.birthdate}:</b> {$profilefields.dateofbirth} |{/if}
					{if $profilefields.citizenship}<b>{$text.nationality}:</b> {$profilefields.citizenship} |{/if}
					{if $profilefields.gender}<b>{$text.gender}:</b> {if $profilefields.gender == 'male'}{$text.man}{/if}{if $profilefields.gender == 'female'}{$text.woman}{/if}{if $profilefields.gender == 'male' && $profilefields.gender == 'female'}{$text.gendernotspecified}{/if} |{/if}
					{if $profilefields.mobilenumber}{$profilefields.mobilenumber} |{/if}
					{if $profilefields.homenumber}{$profilefields.homenumber} |{/if}
					{if $profilefields.businessnumber}{$profilefields.businessnumber} |{/if}
					{if $profilefields.email}<a href="mailto:{$profilefields.email}">{$profilefields.email}</a> |{/if}
					{if $profilefields.address}{$profilefields.address}{/if}{if $profilefields.town}, {$profilefields.town}{/if}{if $profilefields.city}, {$profilefields.city}{/if}{if $profilefields.country}, {str tag='country.$profilefields.country' section='artefact.europass'}{/if}
				</p>
			</td>
		</tr>
		</table>
	</div>

	<div class="content">
		<br /><h3>{$text.employment}</h3>

		{foreach from=$employment_data item=employment}
		<p class="entry">
			<div class="date">{$employment->startdate|escape}{if $employment->enddate} &ndash; {$employment->enddate|escape}{else} &rarr;{/if}</div>
			<div class="label"><b>{$employment->jobtitle}</b> &ndash; {$employment->employer}</div>
			{if $employment->employeraddress}<div class="address">{$employment->employeraddress}</div>{/if}
		</p>
		{/foreach}

		<br /><h3>{$text.education}</h3>

		{foreach from=$education_data item=education}
        <p class="entry">
			<div class="date">{$education->startdate|escape}{if $education->enddate} &ndash; {$education->enddate|escape}{else} &rarr;{/if}</div>
			<div class="label"><b>{$education->qualname}</b> &ndash; {$education->institution}</div>
			{if $education->institutionaddress}<div class="address">{$education->institutionaddress}</div>{/if}
		</p>
        {/foreach}

		<br /><h3>{$text.languageskills}</h3>

		<p class="entry">
			<div>{$text.mothertongue} &ndash; <span class="label">{foreach from=$skills.mothertongue item=lang name=lg}<b>{$lang->label}</b>{if !$.foreach.lg.last} &bull; {/if}{/foreach}</span></div>
			<div>{$text.otherlanguage}</div>
			<br /><table width="100%" class="info-table">
				<tr class="info-table-header">
					<th width="30%">&nbsp;</th>
					<th width="28%" colspan="2">{$text.understanding}</th>
					<th width="28%" colspan="2">{$text.speaking}</th>
					<th width="14%">{$text.writing}</th>
				</tr>
				<tr align="center" class="info-table-header">
					<td width="30%">&nbsp;</td>
					<td width="14%"><small>{$text.listening}</small></td>
					<td width="14%"><small>{$text.reading}</small></td>
					<td width="14%"><small>{$text.spokeninteraction}</small></td>
					<td width="14%"><small>{$text.spokenproduction}</small></td>
					<td width="14%">&nbsp;</td>
				</tr>
				{foreach from=$skills.otherlanguage item=lang}
				<tr align="center">
					<th width="30%" align="left"><span class="label">{$lang->label}</span></th>
					<td width="14%">{$lang->listening}</td>
					<td width="14%">{$lang->reading}</td>
					<td width="14%">{$lang->spokeninteraction}</td>
					<td width="14%">{$lang->spokenproduction}</td>
					<td width="14%">{$lang->writing}</td>
				</tr>
				{/foreach}
				<tr class="info-table-footer">
					<td colspan="6"><small><i>{$text.cef_level}</i></small></td>
				</tr>
			</table>
		</p>

		<br /><h3>{$text.digitalskills}</h3>

		<p class="entry">
			<table width="100%" class="info-table">
				<tr align="center" class="info-table-header">
					<td width="30%">&nbsp;</td>
					<td width="14%"><small>{$text.information}</small></td>
					<td width="14%"><small>{$text.communication}</small></td>
					<td width="14%"><small>{$text.contentcreation}</small></td>
					<td width="14%"><small>{$text.safety}</small></td>
					<td width="14%"><small>{$text.problemsolving}</small></td>
				</tr>
				<tr align="center">
					<th width="30%" align="left"><span class="label">{$text.digitalcompetence}</span></th>
					<td width="14%">{$skills.digital->information}</td>
					<td width="14%">{$skills.digital->communication}</td>
					<td width="14%">{$skills.digital->contentcreation}</td>
					<td width="14%">{$skills.digital->safety}</td>
					<td width="14%">{$skills.digital->problemsolving}</td>
				</tr>
				<tr class="info-table-footer">
					<td colspan="6"><small><i>{$text.digcomp_level}</i></small></td>
				</tr>
			</table>
			<br />
			<div class="skill">{$text.otherdigitalcompetences}</div>
			<div class="details">{$skills.digital->description|safe}</div>
		</p>

		<br /><h3>{$text.skills}</h3>

		{if !empty($skills.skills.socialskill.desc)}
		<p class="entry">
			<div class="skill">{$text.communicationskill}</div>
			<div class="details">{$skills.skills.socialskill.desc|safe}</div>
		</p>
		{/if}

		{if !empty($skills.skills.organisationalskill.desc)}
		<p class="entry">
			<div class="skill">{$text.organisationalskill}</div>
			<div class="details">{$skills.skills.organisationalskill.desc|safe}</div>
		</p>
		{/if}

		{if !empty($skills.skills.technicalskill.desc)}
		<p class="entry">
			<div class="skill">{$text.jobrelatedskill}</div>
			<div class="details">{$skills.skills.technicalskill.desc|safe}</div>
		</p>
		{/if}

		{if !empty($skills.skills.otherskill.desc)}
		<p class="entry">
			<div class="skill">{$text.otherskill}</div>
			<div class="details">{$skills.skills.otherskill.desc|safe}</div>
		</p>
		{/if}

		{if !empty($skills.skills.drivinglicence.desc)}
		<p class="entry">
			<div class="label"><b>{$text.drivinglicence}</b> {$skills.skills.drivinglicence.desc|safe}</div>
		</p>
		{/if}

		<br /><h3>{$text.additionalinfo}</h3>

		{foreach from=$additionalinfo item=item}
		<p class="entry">
			<div class="skill">{$item->label}</div>
			<div class="details">{$item->description|safe}</div>
		</p>
		{/foreach}

	</div>

{if !$embed}
</body>
</html>
{/if}
